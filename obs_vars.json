{
    "r_p":{
        "access":"self.pose[:self.trans_deg]",
        "desc":"Platform position",
        "dtype":"np.ndarray",
        "size":"self.trans_deg"
    },
    "r_p_dot":{
        "access":"self.pose_dot[:self.trans_deg]",
        "desc":"Platform velocity vector",
        "dtype":"np.ndarray",
        "size":"self.trans_deg"
    },
    "r_p_dot_dot":{
        "access":"self.pose_dot_dot[:self.trans_deg]",
        "desc":"Platform acceleration vector",
        "dtype":"np.ndarray",
        "size":"self.trans_deg"
    },
    "x":{
        "access":"self.r_p[self.axes.index('x')]",
        "desc":"x component of position",
        "dtype":"float",
        "size":1
    },
    "y":{
        "access":"self.r_p[self.axes.index('y')]",
        "desc":"y component of position",
        "dtype":"float",
        "size":1
    },
    "z":{
        "access":"self.r_p[self.axes.index('z')]",
        "desc":"z component of position",
        "dtype":"float",
        "size":1
    },
    "v":{
        "access":"np.linalg.norm(self.r_p_dot)",
        "desc":"Platform velocity",
        "dtype":"float",
        "size":1
    },
    "v_x":{
        "access":"self.r_p_dot[self.axes.index('x')]",
        "desc":"x component of velocity vector",
        "dtype":"float",
        "size":1
    },
    "v_y":{
        "access":"self.r_p_dot[self.axes.index('y')]",
        "desc":"y component of velocity vector",
        "dtype":"float",
        "size":1
    },
    "v_z":{
        "access":"self.r_p_dot[self.axes.index('z')]",
        "desc":"z component of velocity vector",
        "dtype":"float",
        "size":1
    },
    "a":{
        "access":"np.linalg.norm(self.r_p_dot_dot)",
        "desc":"Platform accelartion",
        "dtype":"float",
        "size":1
    },
    "a_x":{
        "access":"self.r_p_dot_dot[self.axes.index('x')]",
        "desc":"x component of acceleration vector",
        "dtype":"float",
        "size":1
    },
    "a_y":{
        "access":"self.r_p_dot_dot[self.axes.index('y')]",
        "desc":"y component of acceleration vector",
        "dtype":"float",
        "size":1
    },
    "a_z":{
        "access":"self.r_p_dot_dot[self.axes.index('z')]",
        "desc":"z component of acceleration vector",
        "dtype":"float",
        "size":1
    },
    "Phi":{
        "access":"self.pose[-self.rot_deg:]",
        "desc":"Orientation of platform",
        "dtype":"np.ndarray",
        "size":"self.rot_deg"
    },
    "Phi_dot":{
        "access":"self.pose_dot[-self.rot_deg:]",
        "desc":"Orientation of platform",
        "dtype":"np.ndarray",
        "size":"self.rot_deg"
    },
    "Phi_dot_dot":{
        "access":"self.pose_dot_dot[-self.rot_deg:]",
        "desc":"Orientation of platform",
        "dtype":"np.ndarray",
        "size":"self.rot_deg"
    },
    "phi":{
        "access":"self.Phi[self.axes.index('x')]",
        "desc":"angle around x axis",
        "dtype":"float",
        "size":1
    },
    "phi_dot":{
        "access":"self.Phi_dot[self.axes.index('x')]",
        "desc":"angular velocity around x axis",
        "dtype":"float",
        "size":1
    },
    "phi_dot_dot":{
        "access":"self.Phi_dot_dot[self.axes.index('x')]",
        "desc":"angular acceleration around x axis",
        "dtype":"float",
        "size":1
    },
    "theta":{
        "access":"self.Phi[self.axes.index('y')]",
        "desc":"angle around y axis",
        "dtype":"float",
        "size":1
    },
    "theta_dot":{
        "access":"self.Phi_dot[self.axes.index('y')]",
        "desc":"angular velocity around y axis",
        "dtype":"float",
        "size":1
    },
    "theta_dot_dot":{
        "access":"self.Phi_dot_dot[self.axes.index('y')]",
        "desc":"angular acceleration around y axis",
        "dtype":"float",
        "size":1
    },
    "psi":{
        "access":"self.Phi[self.axes.index('z')]",
        "desc":"angle around z axis",
        "dtype":"float",
        "size":1
    },
    "psi_dot":{
        "access":"self.Phi_dot[self.axes.index('z')]",
        "desc":"angular velocity around z axis",
        "dtype":"float",
        "size":1
    },
    "psi_dot_dot":{
        "access":"self.Phi_dot_dot[self.axes.index('z')]",
        "desc":"angular acceleration around z axis",
        "dtype":"float",
        "size":1
    },
    "e":{
        "access":"self.goal-self.r_p",
        "desc":"Positional error vector",
        "dtype":"np.ndarray",
        "size":"self.trans_deg"
    },
    "e_abs":{
        "access":"np.linalg.norm(self.e)",
        "desc":"Absolute positional error",
        "dtype":"np.ndarray",
        "size":"self.trans_deg"
    },
    "e_dot_abs":{
        "access":"np.linalg.norm(self.e)",
        "desc":"Absolute positional error rate",
        "dtype":"float",
        "size":1
    },
    "f":{
        "access":"self.f",
        "desc":"Cable forces",
        "dtype":"np.ndarray",
        "size":"self.m"
    },
    "delta_f":{
        "access":"self.f-self.f_prev",
        "desc":"cable_force delta",
        "dtype":"np.ndarray",
        "size":"self.m"
    }
}